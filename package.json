{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"start": "svelte-kit start",
		"adapt": "svelte-kit adapt --verbose",
		"test": "jest"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-typescript": "^7.12.7",
		"@snowpack/plugin-typescript": "^1.0.0",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/snowpack-config": "next",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/svelte": "^3.0.3",
		"@types/jest": "^26.0.20",
		"babel-jest": "^26.6.3",
		"d3": "^6.5.0",
		"jest": "^26.6.3",
		"lodash": "^4.17.20",
		"sass": "^1.0.0",
		"surge": "^0.21.7",
		"svelte": "^3.29.0",
		"svelte-jester": "^1.3.0",
		"svelte-preprocess": "^4.0.0",
		"ts-jest": "^26.4.4",
		"tslib": "^2.0.0",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.0",
		"axios": "^0.21.1",
		"node-fetch": "^2.6.1",
		"patch-package": "^6.2.2"
	},
	"jest": {
		"transform": {
			"^.+\\.svelte$": [
				"svelte-jester",
				{
					"preprocess": true
				}
			],
			"^.+\\.ts$": "ts-jest",
			"^.+\\.js$": "babel-jest"
		},
		"moduleFileExtensions": [
			"js",
			"ts",
			"svelte"
		]
	}
}
